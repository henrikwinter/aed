<div class="row">

    <div class="col-1">
 
    </div>
    <div class="col-10">

        @Html.Partial((String)ViewBag.Appselector+"/_Partial"+ (String)ViewBag.Appselector+"Home")

    </div>
    <div class="col-1"></div>
</div>


