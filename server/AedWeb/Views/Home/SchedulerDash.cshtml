@using Dextra.Helpers
@model Xapp.Models.FunctionViewModel
@{
    ViewBag.FormId = "Fid_HomeSchedulerDash";
    ViewBag.Title = ViewBag.FormId;
    Layout = "~/Views/Shared/_Layout" + ViewBag.Layout + ".cshtml";
}
@section scripts {
    @Html.ServerSideInclude("~/Views/Home/SchedulerDash.js")
}
@helper L(string title)
{
    @Html.Lang(title, (string)ViewBag.FormId);
}

<div class="row">
    <div class="col-md-2"></div>
    <div class="col-md-10">
        <div id="scheduler"></div>
    </div>
    <div class="col-md-2"></div>
</div>
